{
    "name": "phpactor/php-extension",
    "description": "Description about my project",
    "license": "MIT",
    "type": "phpactor-extension",
    "authors": [
        {
            "name": "daniel@dantleech.com",
            "email": "daniel@dantleech.com"
        }
    ],

    "require": {
        "phpactor/container": "^1.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.25",
        "phpunit/phpunit": "^9.1",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "Phpactor\\Extension\\PhpExtension\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpactor\\Extension\\PhpExtension\\Tests\\": "tests/"
        }
    },
    "extra": {
        "phpactor.extension_class": "Phpactor\\Extension\\PhpExtension\\PhpExtensionExtension",
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
